<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SsInstitucionFormadora" table="ss_institucion_formadora">
    <unique-constraints>
      <unique-constraint name="ss_institucion_formadora_pk" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="150" nullable="true"/>
    <field name="habilitada" type="boolean" column="habilitada" nullable="true"/>
  </entity>
</doctrine-mapping>
