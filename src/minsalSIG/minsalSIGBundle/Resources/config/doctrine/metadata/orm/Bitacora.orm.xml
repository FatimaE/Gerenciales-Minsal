<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Bitacora" table="bitacora">
    <id name="idBitacora" type="integer" column="id_bitacora">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="usuario" type="integer" column="usuario" nullable="false"/>
    <field name="tabla" type="string" column="tabla" length="50" nullable="false"/>
    <field name="operacion" type="string" column="operacion" length="50" nullable="false"/>
    <field name="fecha" type="date" column="fecha" nullable="false"/>
  </entity>
</doctrine-mapping>
